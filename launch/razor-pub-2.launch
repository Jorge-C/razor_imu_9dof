<launch>
  <node pkg="razor_imu_9dof" type="imu_node.py" name="stewart_imu_node" output="screen">
    <rosparam file="$(find razor_imu_9dof)/config/stewart.yaml" command="load"/>
    <remap from="imu" to="stewart_platform/imu"/>
    <remap from="diagnostics" to="stewart_platform/imu/diagnostics"/>
  </node>
  <node pkg="razor_imu_9dof" type="imu_node.py" name="jaguar_imu_node" output="screen">
    <rosparam file="$(find razor_imu_9dof)/config/jaguar.yaml" command="load"/>
    <remap from="imu" to="jaguar/imu"/>
    <remap from="diagnostics" to="jaguar/imu/diagnostics"/>
  </node>
</launch>
